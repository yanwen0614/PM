<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <configSections>
        <sectionGroup name="userSettings" type="System.Configuration.UserSettingsGroup, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089">
            <section name="PM.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" allowExeDefinition="MachineToLocalUser" requirePermission="false" />
        </sectionGroup>
      <section name="log4net" type="System.Configuration.IgnoreSectionHandler" />
    </configSections>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
    </startup>

  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Newtonsoft.Json" publicKeyToken="30ad4fe6b2a6aeed" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-11.0.0.0" newVersion="11.0.0.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
  <userSettings>
    <PM.Properties.Settings>
      <setting name="数据库名称" serializeAs="String">
        <value>locationinfor</value>
      </setting>
      <setting name="数据库用户" serializeAs="String">
        <value>yanwen1</value>
      </setting>
      <setting name="数据库密码" serializeAs="String">
        <value>duoduo</value>
      </setting>
      <setting name="数据库地址" serializeAs="String">
        <value>localhost</value>
      </setting>
    </PM.Properties.Settings>
  </userSettings>
  <appSettings>
    <add key="CnebCrawler" value="false" />
    <add key="IfengCrawler" value="true" />
    <add key="News163Crawler" value="false" />
    <add key="SinachinaCrawler" value="false" />
    <add key="SouhuCrawler" value="false" />
    <add key="PeopleCrawler" value="false" />
    <add key="WeixingongzhongCrawler" value="false" />
    <add key="XinhuanetCrawler" value="false" />
    <add key="Isvisible" value="false" />
  </appSettings>
  <system.data>
    <DbProviderFactories>
      <remove invariant="MySql.Data.MySqlClient" />
      <add description=".Net Framework Data Provider for MySQL" invariant="MySql.Data.MySqlClient" name="MySQL Data Provider" type="MySql.Data.MySqlClient.MySqlClientFactory, MySql.Data, Version=6.10.6.0, Culture=neutral, PublicKeyToken=c5687fc88969c44d" />
    </DbProviderFactories>
  </system.data>
  <log4net>
    <!--定义输出到文件中-->
    <appender name="LogFileAppender" type="log4net.Appender.RollingFileAppender">
      <!--定义文件存放位置-->
      <file value="logfile.txt" />
      <!--如果放在Debug下,当然名字你可以改 -->
      <!--<file value="log//logfile.txt"/>-->
      <appendToFile value="true" />
      <rollingStyle value="Date" />
      <datePattern value="yyyyMMdd-HH:mm:ss" />
      <layout type="log4net.Layout.PatternLayout">
        <!--输出格式-->
        <!--样例：2008-03-26 13:42:32,111 [10] INFO  Log4NetDemo.MainClass [(null)] - info-->
        <conversionPattern value="记录时间：%date 线程ID:[%thread] 日志级别：%-5level 出错类：%logger property:[%property{NDC}] - 错误描述：%message%newline" />
      </layout>
    </appender>
    <!--定义日志的输出媒介-->
    <root>
      <!--文件形式记录日志-->
      <appender-ref ref="LogFileAppender" />
    </root>
  </log4net>
</configuration>